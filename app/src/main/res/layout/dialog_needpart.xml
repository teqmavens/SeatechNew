                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/verysmallpadding"
                    app:layout_constraintLeft_toLeftOf="@+id/checkbox_yesurgent"
                    app:layout_constraintTop_toTopOf="@+id/uploadimage"
                    app:srcCompat="@drawable/uploadfile" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/additionalnotes"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/largetextsize"
                    android:text="@string/additionalnotes"
                    android:textColor="@color/black"
                    android:textSize="@dimen/verysmalltextsize"
                    android:textStyle="bold"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toLeftOf="@+id/cbxneedquote_yes"
                    app:layout_constraintTop_toBottomOf="@+id/uploadimage" />


                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/et_additnotes"
                    android:layout_width="0dp"
                    android:layout_height="80dp"
                    android:layout_marginTop="@dimen/smallpadding"
                    android:background="@drawable/dashboardlinebg"
                    android:gravity="top"
                    android:padding="@dimen/verysmalltextsize"
                    android:textColor="@color/black"
                    android:textColorHint="@color/black"
                    android:textSize="@dimen/verysmalltextsize"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/additionalnotes" />


                <android.support.v7.widget.AppCompatButton
                    android:id="@+id/savebtn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/smalltextsize"
                    android:background="@drawable/bg_loginbtn"
                    android:paddingLeft="@dimen/smallpadding"
                    android:paddingRight="@dimen/smallpadding"
                    android:text="@string/save"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="@dimen/verysmalltextsize"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toLeftOf="@+id/addmorepartsbtn"
                    app:layout_constraintTop_toBottomOf="@+id/et_additnotes" />

                <android.support.v7.widget.AppCompatButton
                    android:id="@+id/addmorepartsbtn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_loginbtn"
                    android:paddingLeft="@dimen/smallpadding"
                    android:paddingRight="@dimen/smallpadding"
                    android:text="@string/addmorepart"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="@dimen/verysmalltextsize"
                    app:layout_constraintLeft_toRightOf="@+id/savebtn"
                    app:layout_constraintRight_toLeftOf="@+id/cancelbtn"
                    app:layout_constraintTop_toTopOf="@+id/savebtn" />

                <android.support.v7.widget.AppCompatButton
                    android:id="@+id/cancelbtn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_loginbtn"
                    android:paddingLeft="@dimen/smallpadding"
                    android:paddingRight="@dimen/smallpadding"
                    android:text="@string/cancel"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="@dimen/verysmalltextsize"
                    android:onClick="@{()->dialogneedpart.OnClickCancel()}"
                    app:layout_constraintLeft_toRightOf="@+id/addmorepartsbtn"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/savebtn" />


            </android.support.constraint.ConstraintLayout>
                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/cross"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/crosspopup"
                    android:onClick="@{()->dialogneedpart.onClickCross()}"/>
            </android.support.constraint.ConstraintLayout>
        </LinearLayout>
    </ScrollView>
</layout>